Cell[BoxData[{
 RowBox[{
  RowBox[{"partitionThatString", "[", "x_", "]"}], ":=", 
  RowBox[{"StringPartition", "[", 
   RowBox[{"x", ",", "3"}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"convertPaddedStringToString", "[", "x_", "]"}], ":=", 
  RowBox[{"FromCharacterCode", "[", "x", "]"}]}], "\n", 
 RowBox[{"fixPadBillStyle", ":=", " ", 
  RowBox[{
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"StringLength", " ", "[", " ", "#", " ", "]"}], " ", "~", " ", 
       "Mod", " ", "~", " ", "3"}], " ", "\[Equal]", " ", "0"}], ",", " ", 
     "#", ",", " ", 
     RowBox[{"StringPadLeft", "[", " ", 
      RowBox[{"#", ",", 
       RowBox[{"3", "*", 
        RowBox[{"Ceiling", "[", 
         FractionBox[
          RowBox[{"(", 
           RowBox[{"StringLength", " ", "@", " ", "#"}], ")"}], "3"], "]"}]}],
        ",", "\"\<0\>\""}], "]"}]}], "]"}], "&"}]}]}], "Input",
 CellChangeTimes->{
