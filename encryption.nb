Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"cpr", "[", 
    RowBox[{"x_Integer", ",", "k_Integer", ",", "m_Integer"}], "]"}], "/;", 
   RowBox[{"x", ">", "m", ">", "k", ">", "1"}]}], ":=", 
  RowBox[{"RandomChoice", "@", 
   RowBox[{"Pick", "[", 
    RowBox[{
     RowBox[{"Range", "[", 
      RowBox[{"k", ",", "m"}], "]"}], ",", 
     RowBox[{"CoprimeQ", "[", 
      RowBox[{"x", ",", 
       RowBox[{"Range", "[", 
        RowBox[{"k", ",", "m"}], "]"}]}], "]"}]}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"generateR", "[", 
   RowBox[{"p_", ",", "q_"}], "]"}], ":=", 
  RowBox[{"cpr", "[", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"p", "-", "1"}], ")"}], "*", 
      RowBox[{"(", 
       RowBox[{"q", "-", "1"}], ")"}]}], ")"}], ",", "2", ",", 
    RowBox[{"q", "-", "1"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"generateS", "[", 
   RowBox[{"r_", ",", "p_", ",", "q_"}], "]"}], ":=", " ", 
  RowBox[{"PowerMod", "[", 
   RowBox[{"r", ",", 
    RowBox[{"-", "1"}], ",", 
    RowBox[{"EulerPhi", "[", 
     RowBox[{"p", "*", "q"}], "]"}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.674433714587347*^9, 3.674433725479597*^9}, {
   3.6744338313097196`*^9, 3.674433886647319*^9}, 3.6744339443654313`*^9, 
   3.6744401150979147`*^9}],

